/**
 * @msg {"Hello"} Hello - The message It calls
 * @color "#00FF00" Color - It calls the Message with a color
*/
print("Hello!", { color: "#00FF00" })

/**
 * @warn {"Aw!"} Aw! - The warn It calls
 * @color "#FFA500" Color - It calls the Warn with a color
*/
warn("Aw!", { color: "#FFA500" })

/**
 * @error {"Something broke!"} Something broke! - Throws the error at the player!
*/
error("Something broke!")

